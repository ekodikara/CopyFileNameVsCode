{
  "name": "copy-filename-extension",
  "description": "Copy the current file name to the clipboard",
  "displayName": "Copy Filename to Clipboard",
  "publisher": "Moonxt | By Kodikara",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "icon": "icon.png",
  "activationEvents": ["onCommand:extension.copyFileName"],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.copyFileName",
        "title": "Copy File Name to Clipboard"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "extension.copyFileName",
          "group": "navigation",
          "when": "resource"
        }
      ],
      "editor/title": [
        {
          "command": "extension.copyFileName",
          "group": "navigation",
          "when": "resource"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "eslint": "^9.16.0",
    "esbuild": "^0.24.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.2",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
  ,"repository": {
    "type": "git",
    "url": "https://github.com/your-repository-url.git"
  },
  "bugs": {
    "url": "https://github.com/your-repository-url/issues"
  },
  "homepage": "https://github.com/your-repository-url"
}
